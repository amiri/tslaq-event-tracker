{
    "presets": [
        "modern-browsers"
    ],
    "plugins": [
        "transform-remove-console",
        [
            "@babel/plugin-transform-runtime",
            {
                "absoluteRuntime": false,
                "corejs": false,
                "helpers": true,
                "regenerator": true,
                "useESModules": false
            }
        ],
        ["import", { "libraryName": "antd", "style": "css" }]
    ]
}
