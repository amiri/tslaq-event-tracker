{
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "antd": "^3.16.5",
    "axios": "^0.18.0",
    "d3": "^5.9.2",
    "d3fc": "^14.0.37",
    "draft-js": "^0.10.5",
    "es6-shim": "^0.35.5",
    "node-fetch": "^2.5.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.0.3",
    "react-scripts": "2.1.8",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "babel-loader": "^8.0.5",
    "eslint-config-xo-react": "^0.19.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-react-hooks": "^1.6.0",
    "esm": "^3.2.22",
    "husky": "^2.1.0",
    "lint-staged": "^8.1.5",
    "prettier": "^1.17.0",
    "pretty-quick": "^1.10.0",
    "xo": "^0.24.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/amiri/tslaq-event-tracker.git"
  },
  "xo": {
    "prettier": true,
    "space": 2,
    "envs": [
      "browser",
      "es6",
      "node"
    ],
    "rules": {
      "import/no-unassigned-import": [
        "error",
        {
          "allow": [
            "**/*.css"
          ]
        }
      ]
    }
  },
  "name": "frontend",
  "private": true,
  "scripts": {
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "lint": "xo --space --prettier --fix",
    "start": "react-scripts start",
    "test": "react-scripts test"
  },
  "version": "0.1.0"
}
